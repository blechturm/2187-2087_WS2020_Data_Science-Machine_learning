\mode<presentation>

\definecolor{craneorange}{RGB}{252,187,6}
\definecolor{craneblue}{RGB}{4,6,76}
%% wu colors
\definecolor{wulightblue}{RGB}{0,153,204} 
\definecolor{wublue}{RGB}{51,102,153} 
\definecolor{wudarkblue}{RGB}{0,51,102} 
\definecolor{wupurple}{RGB}{102,51,153}
\definecolor{wugreen}{RGB}{0,153,153}

\setbeamercolor{structure}{fg=craneblue}
\setbeamercolor{alerted text}{fg=wulightblue}%{fg=wugreen}
\setbeamercolor{example text}{fg=wugreen} 

\setbeamercolor{palette primary}{fg=black,bg=white!70}
\setbeamercolor{palette secondary}{fg=black,bg=white!70}
\setbeamercolor{palette tertiary}{fg=black,bg=white!70}
\setbeamercolor{palette quaternary}{fg=white,bg=wulightblue}

\setbeamercolor{titlelike}{parent=palette quaternary}

% \setbeamercolor{part name}{partent=titlelike}
\setbeamercolor{part title}{parent=title}

\setbeamercolor{author}{fg=white,bg=wulightblue}
\setbeamercolor{logo}{fg=white,bg=wulightblue}
\setbeamercolor{title}{fg=white,bg=wupurple}

\setbeamercolor{block title}{fg=white,bg=wulightblue!70!bg}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg!70!bg}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg!70!bg}

\setbeamercolor{block body}{fg=black,bg=wulightblue!20!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!20!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!20!bg}

\setbeamercolor{sidebar}{bg=wulightblue!70}
  
\setbeamercolor{palette sidebar primary}{fg=craneblue}
\setbeamercolor{palette sidebar secondary}{fg=craneblue!75}
\setbeamercolor{palette sidebar tertiary}{fg=craneblue!75}
\setbeamercolor{palette sidebar quaternary}{fg=craneblue}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamercolor{itemize item}{fg=wulightblue}
\setbeamercolor{itemize subitem}{parent=itemize item}
\setbeamercolor{itemize subsubitem}{parent=itemize subitem}

\setbeamercolor{section in toc}{fg=black,bg=white}
\setbeamercolor{subsection in toc}{parent=section in toc}
\setbeamercolor{subsubsection in toc}{parent=subsection in toc}

\setbeamercolor{enumerate item}{fg=wulightblue}

\setbeamercolor{footlineelement}{fg=white,bg=wupurple}

\mode
<all>
